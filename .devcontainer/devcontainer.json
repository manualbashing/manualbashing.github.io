{
	"name": "manualbashing",
	"dockerFile": "Dockerfile",
	"appPort": 4000,
	"extensions": [],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
    "postCreateCommand": ["bundle", "install"],
    "postAttachCommand": ["bundle", "exec", "jekyll", "serve", "--host", "0.0.0.0", "--detach"]
}